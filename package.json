{
  "name": "nest-neogma",
  "version": "0.6.0",
  "description": "Neogma integration for Nest",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --quiet src/**/*.ts",
    "format": "prettier --log-level silent --write src/**/*.ts",
    "build": "rm -rf dist && yarn lint && yarn format && tsc",
    "test": "jest"
  },
  "repository": "git+https://github.com/AndySakov/nest-neogma.git",
  "keywords": [
    "nestjs",
    "nest",
    "ogm",
    "neo4j",
    "neogma"
  ],
  "author": "Obafemi Teminife <andysakov1958@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AndySakov/nest-neogma/issues"
  },
  "homepage": "https://github.com/AndySakov/nest-neogma#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/testing": "^10.4.15",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "node-notifier": "^10.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "neo4j-driver": "^5.27.0",
    "neogma": "^1.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  }
}
